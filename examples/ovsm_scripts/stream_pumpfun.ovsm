            (if (string-contains line "sol_received:")
                (do
                  ;; Parse: "Program log: sol_received: 5630603, fee_amount: 56306"
                  (define parts (split line ":"))
                  (if (>= (length parts) 3)
                      (do
                        ;; Get amount part (index 2)
                        (define amount-str (get parts 2))
                        (define amount-parts (split amount-str ","))
                        (if (>= (length amount-parts) 1)
                            (do
                              (set! sol-amount (get amount-parts 0))
                              ;; Format as SOL (divide by 1e9)
                              ;; For display: "5630603 lamports (~0.0056 SOL)"
                              (set! sol-amount-formatted (str sol-amount " lamports")))
                            null)

                        ;; Get fee if present (index 3)
                        (if (>= (length parts) 4)
                            (set! fee-amount (get parts 3))
                            null))
                      null))
                null))

