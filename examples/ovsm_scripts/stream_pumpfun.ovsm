;;; Stream Pump.fun Events - ULTIMATE FORENSICS PARSER
;;;
;;; This script provides comprehensive blockchain forensics for Pump.fun:
;;; - Full transaction signatures (not truncated)
;;; - Wallet addresses (from signer/invoke)
;;; - Program IDs and token types
;;; - SOL amounts (when available)
;;; - Token mint addresses
;;; - Processing latency (current time - event time)
;;; - Direct Solscan links
;;;
;;; Usage:
;;;   1. Start streaming server: osvm stream --programs pumpfun --port 8080
;;;   2. Run this script: osvm ovsm run stream_pumpfun.ovsm

;; Connect to streaming server (local instance)
(define stream-id (stream-connect "http://localhost:8080"
                                  :programs ["pumpfun"]))

(log :message "âœ… Connected to Pump.fun event stream" :value stream-id)
(log :message "ğŸ” ULTIMATE FORENSICS PARSER - Full transaction intelligence")
(log :message "")

;; Initialize counters and tracking
(define buy-count 0)
(define sell-count 0)
(define graduation-count 0)
(define total-sol-volume 0)
(define start-monitoring-time (now))

;; Main event loop (run for 60 seconds)
(define start-time (now))
(define duration 60)

(while (< (- (now) start-time) duration)
  ;; Poll for new events (non-blocking, returns array)
  (define events (stream-poll stream-id :limit 50))

  ;; Process each event (both Transaction and LogMessage)
  (for (event events)
    (define event-type (get event "type"))

    ;; Process Transaction events for wallet/signer info
    (if (= event-type "transaction")
        (do
          (define signature (get event "signature"))
          (define signer (get event "signer"))
          (define slot (get event "slot"))
          (define fee (get event "fee"))
          (define timestamp (get event "timestamp"))
          (define processing-time (now))

          ;; For now, just log that we received it
          ;; In full implementation, we'd correlate with LogMessage events
          null)
        null)

    ;; Process log_message events for transaction details
    (if (= event-type "log_message")
        (do
          (define logs (get event "logs"))
          (define signature (get event "signature"))
          (define slot (get event "slot"))
          (define processing-time (now))

          ;; Convert logs to searchable text
          (define logs-text (str logs))

          ;; â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
          ;; EXTRACT WALLET from logs (look for base58 addresses)
          ;; â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
          (define wallet "View on osvm.ai")
          ;; First log line often contains signer context
          (if (> (length logs) 0)
              (do
                (define first-log (get logs 0))
                ;; Look for addresses in invoke statements
                (for (line logs)
                  (if (and (string-contains line "invoke [1]")
                           (not (string-contains line "ComputeBudget"))
                           (not (string-contains line "11111111")))
                      (do
                        ;; The pattern is: "Program <ADDRESS> invoke [1]"
                        (define parts (split line " "))
                        (if (>= (length parts) 2)
                            (do
                              ;; parts[1] is the wallet/program that was invoked
                              (define potential-wallet (get parts 1))
                              ;; Only set if it looks like a base58 address (length check)
                              (if (> (length potential-wallet) 32)
                                  (set! wallet potential-wallet)
                                  null))
                            null))
                      null)))
              null)

          ;; â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
