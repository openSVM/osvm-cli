CONST PUMPFUN = "6EF8rrecthR5Dkzon8Nwu78hRvfCKubJ14M5uBEwF6P"

$opts = {"address": PUMPFUN, "limit": 1000}
$batch = getSignaturesForAddress($opts)

// Get first and last transaction times
$first = $batch[0]
$last_idx = COUNT($batch) - 1
$last = $batch[$last_idx]

RETURN {"count": COUNT($batch), "newest_time": $first.blockTime, "oldest_time": $last.blockTime, "time_span_seconds": $first.blockTime - $last.blockTime}
