// Complex conditional logic
$score = 85

IF $score >= 90 THEN
    RETURN "Grade: A - Excellent!"
ELSE
    IF $score >= 80 THEN
        RETURN "Grade: B - Good job!"
    ELSE
        IF $score >= 70 THEN
            RETURN "Grade: C - Average"
        ELSE
            IF $score >= 60 THEN
                RETURN "Grade: D - Needs improvement"
            ELSE
                RETURN "Grade: F - Failed"
